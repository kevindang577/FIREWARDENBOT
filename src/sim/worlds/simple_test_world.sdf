      <link name="tree_link">
        <collision name="tree_collision">
          <geometry>
            <cylinder>
              <radius>0.3</radius>
              <length>2</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="tree_visual">
          <geometry>
            <cylinder>
              <radius>0.3</radius>
